class Hello {
    public static void main(String a[]) {
     int num1 = 7;
     int num2 = 4;
     int result = num1+num2;
     int res =num1*num2;
     int re =num1/num2;
     int r = num1-num2;
      System.out.println(result);
        System.out.println(res);
        System.out.println(re);
        System.out.println(r);

    }
}
