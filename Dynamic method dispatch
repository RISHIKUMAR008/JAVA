import java.util.*;
class  A
{
    public static void show()
    {
        System.out.println(("in A show"));
    }

}
class B extends A{
    public static void show()
    {
        System.out.println("hello B");
    }

}

public class Main {
    public static void main(String a[]) {
        A obj = new A();
        obj.show();
        obj = new B();
        obj.show();

    }
}
