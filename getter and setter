import java.util.*;

class Human {
    private int age;
    private String name;

    public void setAge(int age) {
        this.age = age;
    }

    public int getAge() {
        return age;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}

public class Main {
    public static void main(String args[]) {
        Human obj = new Human();
        // Set age and name using the setter methods
        obj.setAge(11);
        obj.setName("rishi");

        // Retrieve and print age and name using the getter methods
        System.out.println(obj.getName());
        System.out.println(obj.getAge());
    }
}
